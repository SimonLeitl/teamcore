{
  "name": "teamcore",
  "version": "1.0.0",
  "description": "TeamCore is a modular platform that connects playtime data with bonus and travel expense management for sports teams",
  "private": true,
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "migrate:create": "node scripts/migrate.js create",
    "migrate:list": "node scripts/migrate.js list",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:diff": "supabase db diff --use-migra",
    "db:status": "supabase migration list"
  },
  "keywords": [
    "sports",
    "team-management",
    "serverless"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "@vercel/node": "^5.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "supabase": "^1.200.3",
    "typescript": "^5.3.0",
    "vercel": "^50.1.5"
  }
}
